<h1>タスク一覧</h1>


<table class="table table-striped table-bordered">
    <tr>
        <th>id</th>
        <th>ステータス</th>
        <th>タスク</th>
    </tr>
    <% @tasks.each do |task| %>
    <tr>
        <td><%=link_to task.id, task %></td>
        <td><%= task.status %></td>
        <td><%= task.content %></td>
    </tr>
     <% end %>
</table>
    
   

